{
  "name": "rdeco",
  "version": "1.0.0",
  "description": "基于响应式的应用开发框架",
  "private": true,
  "keywords": [
    "react",
    "flux",
    "hook",
    "state",
    "useReducer",
    "rxjs"
  ],
  "licenses": "MIT",
  "author": "shihong <112365362@qq.com>",
  "repository": {
    "type": "git",
